{
  "name": "presto-js-client",
  "description": "JavaScript client for Presto",
  "keywords": [
    "javascript",
    "presto",
    "prestodb",
    "typescript"
  ],
  "license": "Apache-2.0",
  "repository": "https://github.com/prestodb/presto-js-client",
  "scripts": {
    "build": "nx build",
    "build:presto-client": "nx build presto-client",
    "commit": "cz",
    "format": "nx format",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "lint": "nx lint",
    "prepare": "husky install",
    "presto:up": "docker compose -f ./docker-compose/presto/docker-compose.yml -p presto up",
    "publish": "nx publish",
    "serve": "nx serve",
    "test": "nx test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@swc/helpers": "~0.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~17.0.0",
    "@commitlint/config-angular": "~17.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/jest": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/nest": "16.8.1",
    "@nx/node": "16.8.1",
    "@nx/web": "16.8.1",
    "@nx/webpack": "16.8.1",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@swc/jest": "0.2.20",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "commitizen": "~4.3.0",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "~8.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "16.8.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "verdaccio": "^5.0.4"
  },
  "engines": {
    "npm": ">=9.6",
    "node": ">=18.17"
  },
  "private": true,
  "workspaces": [
    "packages/*"
